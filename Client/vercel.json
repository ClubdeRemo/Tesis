{
  "version": 2,
  "public": true,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client/browser",
  "routes": [
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|svg|webp|woff2?|mp4))",
      "headers": { "Cache-Control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
